      </svg>  
    </use>
  </use>
  <svg id="battery-container">
    <use id="battery-symbol" href="#battery"/>
    <svg id="battery-text">
      <image/>
      <image />
      <image />
      <image />
    </svg>
  </svg>
  <svg class="stats-container">
    <use id="steps" href="#activity-symbol" class="steps stat">
      <set href="image" attributeName="href" to="images/stat_steps_solid_48px.png"/>
    </use>
    <use id="calories" href="#activity-symbol" class="calories stat">
      <set href="image" attributeName="href" to="images/stat_cals_solid_48px.png"/>
    </use>
    <use id="activesminutes" href="#activity-symbol" class="activesminutes stat">
      <set href="image" attributeName="href" to="images/stat_azm_solid_48px.png"/>
    </use>
    <use id="distance" href="#activity-symbol" class="distance stat">
      <set href="image" attributeName="href" to="images/stat_dist_solid_48px.png"/>
    </use> 
    <use id="elevation" href="#activity-symbol" class="elevation stat">
      <set href="image" attributeName="href" to="images/stat_floors_solid_48px.png"/>
    </use>
  </svg>
</svg>
